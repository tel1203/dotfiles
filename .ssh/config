Host nictssh
    HostName ssh.nict.go.jp
    User ytel
    IdentityFile ~tel/.ssh/id_rsa
    Port 22
    TCPKeepAlive yes
    IdentitiesOnly yes
    LocalForward   3128 proxy.nict.go.jp:3128
    LocalForward   993  mail.nict.go.jp:993
    LocalForward   465  mail.nict.go.jp:465

